{
	"name": "svelte-lucia-hasura",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "concurrently \"vite dev\" \"pnpm codegen\"",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"hasura:console": "cd hasura && hasura console --admin-secret replacemewithagoodsecret",
		"codegen": "graphql-codegen --config codegen.ts --watch"
	},
	"devDependencies": {
		"@graphql-codegen/cli": "5.0.0",
		"@graphql-codegen/client-preset": "^4.1.0",
		"@graphql-codegen/gql-tag-operations-preset": "^2.1.0",
		"@graphql-codegen/typescript": "4.0.1",
		"@graphql-codegen/typescript-document-nodes": "4.0.1",
		"@parcel/watcher": "^2.3.0",
		"@skeletonlabs/skeleton": "^2.6.0",
		"@skeletonlabs/tw-plugin": "^0.3.0",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.27.4",
		"@tailwindcss/forms": "^0.5.7",
		"@types/node": "^20.10.4",
		"@types/uuid": "^9.0.7",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"autoprefixer": "^10.4.16",
		"concurrently": "^8.2.2",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.30.0",
		"postcss": "^8.4.32",
		"postcss-load-config": "^5.0.2",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"prettier-plugin-tailwindcss": "^0.5.9",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tailwindcss": "^3.3.6",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.4.2"
	},
	"type": "module",
	"dependencies": {
		"@graphql-typed-document-node/core": "^3.2.0",
		"@lucia-auth/adapter-postgresql": "^2.0.2",
		"@urql/svelte": "^4.0.4",
		"graphql": "^16.8.1",
